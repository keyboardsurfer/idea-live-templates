  <template name="activityEmptyView" value="$emptyViewVariable$ = ($WidgetClass$) findViewById(android.R.id.empty);" description="Adds  Empty View in Java" toReformat="true" toShortenFQNames="true">
    <variable name="emptyViewVariable" expression="" defaultValue="emptyView" alwaysStopAt="true" />
